        </ImageView>
        <TextField id="electedTillText" fx:id="netWorthText" layoutX="55.0" layoutY="190.0" prefHeight="29.0" prefWidth="120.0" promptText="NetWorth" style="-fx-background-color: #FEE715FF;" />
        <ImageView fitHeight="75.0" fitWidth="75.0" layoutX="78.0" layoutY="48.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../Images/president.jpeg" />
            </image>
        </ImageView>
      <Button id="registerPresident" fx:id="registerPresident" layoutX="439.0" layoutY="269.0" mnemonicParsing="false" onAction="#PresidentRegister" prefHeight="26.0" prefWidth="90.0" style="-fx-background-color: #FEE715FF;" text="Register" />
      <DatePicker fx:id="electedTillDate" layoutX="55.0" layoutY="148.0" prefHeight="26.0" prefWidth="120.0" promptText="Elected Till" />
